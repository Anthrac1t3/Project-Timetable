      </ImageView>
      <ImageView fitHeight="238.0" fitWidth="215.0" layoutX="331.0" layoutY="47.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Photos/PictureOneGirlYellow.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="246.0" fitWidth="214.0" layoutX="412.0" layoutY="285.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Photos/guyRedHoodie.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="200.0" fitWidth="200.0" layoutX="819.0" layoutY="59.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Photos/Logo.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="109.0" fitWidth="341.0" layoutX="767.0" layoutY="257.0" pickOnBounds="true" preserveRatio="true">
