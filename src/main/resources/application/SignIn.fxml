      </TextField>
      <PasswordField fx:id="passwordField" layoutX="773.0" layoutY="313.0" prefHeight="33.0" prefWidth="234.0" promptText="password" style="-fx-background-color: transparent; -fx-prompt-text-fill: #979797; -fx-text-fill: #979797;">
