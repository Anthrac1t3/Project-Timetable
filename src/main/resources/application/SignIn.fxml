            <Font size="17.0" />
         </font>
      </PasswordField>
      <Button fx:id="signUpButton" layoutX="893.0" layoutY="349.0" mnemonicParsing="false" onAction="#handle" prefHeight="44.0" prefWidth="99.0" style="-fx-background-color: transparent; -fx-text-fill: #FFFFFF;" text="Sign Up">
