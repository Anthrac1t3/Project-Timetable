<AnchorPane fx:id="loginWindow" prefHeight="650.0" prefWidth="1200.0" style="-fx-background-color: #17171A;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.SignInController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="200.0" layoutX="100.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
