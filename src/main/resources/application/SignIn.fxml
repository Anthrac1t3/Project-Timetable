      <TextField fx:id="emailField" layoutX="773.0" layoutY="262.0" prefHeight="33.0" prefWidth="234.0" promptText="email" style="-fx-background-color: transparent; -fx-prompt-text-fill: #979797; -fx-text-fill: #979797;">
